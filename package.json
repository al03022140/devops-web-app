{
  "name": "dev-ops",
  "version": "1.0.0",
  "description": "Aplicación web de gestión de usuarios con roles y permisos (DevOps)",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "data:gen": "node scripts/generate-data.js --avisos=1000 --comments=2 --users=25 --weeks=8 --truncate=true",
    "data:gen:small": "node scripts/generate-data.js --avisos=200 --comments=2 --users=10 --weeks=4 --truncate=true",
    "data:gen:large": "node scripts/generate-data.js --avisos=10000 --comments=3 --users=100 --weeks=12 --truncate=true",
    "bench:metricas": "node scripts/benchmark-metricas.js --iterations=5 --concurrent=1 --warmup=1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.7",
    "prom-client": "^15.1.1",
    "sequelize": "^6.37.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  }
}
